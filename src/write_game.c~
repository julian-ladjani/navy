void	write_map(char **map)
{
  int	i;

  i = 0;
  my_printf(" |A B C D E F G H\n");
  my_printf("-+---------------\n");
  while (map[i] != NULL)
    {
      my_printf("%d|%s\n", i + 1, map[i]);
      i++;
    }
}
